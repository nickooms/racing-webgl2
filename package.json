{
  "name": "racing-webgl2",
  "version": "0.3.0",
  "description": "Racing with WebGL2",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "eslint": "eslint src",
    "server": "SET NODE_ENV=dev&& nodemon -i cache server.js",
    "test": "mocha --timeout 10000",
    "test-eslint": "npm run eslint && npm test"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1,
      "linebreak-style": 0,
      "no-continue": 0,
      "no-plusplus": 0
    }
  },
  "repository": "nickooms/racing-webgl2",
  "author": "Nick Ooms <oomsni@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.16.0",
    "canvas": "^1.6.2",
    "compression": "^1.5.2",
    "config": "^1.24.0",
    "cors": "^2.7.1",
    "earcut": "^2.1.1",
    "express": "^4.13.3",
    "fmt-obj": "^1.1.1",
    "htmlparser2": "^3.9.2",
    "mongoose": "^4.7.7",
    "node-cache": "^4.1.1",
    "node-fetch": "^1.6.3",
    "persistent-cache": "^1.1.1",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.9.2"
  }
}
