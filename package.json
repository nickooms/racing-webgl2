{
  "name": "racing-webgl2",
  "version": "0.3.0",
  "description": "Racing with WebGL2",
  "main": "dist",
  "scripts": {
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test --env=jsdom",
    "react:eject": "react-scripts eject",
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "eslint": "eslint src",
    "server": "NODE_ENV=dev&& nodemon -i cache server.js",
    "babel-server": "nodemon --exec npm run babel-node -- server.js",
    "test": "mocha --timeout 10000",
    "test-eslint": "npm run eslint && npm test",
    "babel-node": ".\\node_modules\\.bin\\babel-node --ignore='cache'"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1,
      "linebreak-style": 0,
      "no-continue": 0,
      "no-plusplus": 0
    }
  },
  "repository": "nickooms/racing-webgl2",
  "author": "Nick Ooms <oomsni@gmail.com>",
  "proxy": "http://localhost:8080",
  "license": "MIT",
  "dependencies": {
    "ansi-styles": "^3.0.0",
    "body-parser": "^1.16.0",
    "canvas": "^1.6.2",
    "chalk": "^1.1.3",
    "compression": "^1.5.2",
    "config": "^1.24.0",
    "cors": "^2.7.1",
    "earcut": "^2.1.1",
    "express": "^4.13.3",
    "fmt-obj": "^1.1.1",
    "htmlparser2": "^3.9.2",
    "material-ui": "^0.17.1",
    "mongoose": "^4.7.7",
    "mongoose-geojson-schema": "^2.1.2",
    "morgan": "^1.8.0",
    "node-cache": "^4.1.1",
    "node-fetch": "^1.6.3",
    "persistent-cache": "^1.1.1",
    "react-router-redux": "^4.0.8",
    "react-swipeable-views": "^0.11.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "resource-router-middleware": "^0.6.0",
    "xml2js": "^0.4.17",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.1.11",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-scripts": "0.9.5"
  }
}
